{
  "dataDirs": {
    "data": "data",
    "code": "code",
    "cache": {
		"path" : "cache",
		"hidden" : true
	},
    "actions": {
		"path" : "actions",
		"hidden" : true
	}
  },

  "permissions": 1,
  "actions": {
    "cpuLoad": {
      "parameters": [],
      "permissions" : 0,
      "js": "cpuLoad",
      "voidAction": true
    },
	"ping" : {
	  "parameters" : [
         {
		   "text" : "-c 100 www.google.com"
		 }
	  ],
	  "command" : "ping",
      "voidAction": true
	},
    "compress_to_zip": {
      "parameters": [
        {
          "name": "output_zip",
          "type": "string",
          "required": true
        },
        {
          "text": "-rj "
        },
        {
          "name": "input_file_list",
          "type": "string",
          "required": true
        }
      ],
      "command": "zip",
      "voidAction": true
    },
    "pwd": {
      "parameters": [
        {
          "text": " "
        }
      ],
      "command": "pwd"
    },
    "set_permissions": {
      "parameters": [
        {
          "name": "directory",
          "type": "file",
          "required": true
        },
        {
          "text": "-R"
        }
      ],
      "executable": "chmod g+rw",
      "voidAction": true
    },
    "sleep": {
      "parameters": [
        {
          "name": "time_in_seconds",
          "type": "int",
          "defaultValue": "2",
          "required": true
        }
      ],
      "command": "sleep",
      "voidAction": true
    },
    "delete_file": {
      "parameters": [
        {
          "name": "file_name",
          "type": "file",
          "required": true
        }
      ],
      "command": "rm",
      "voidAction": true
    },
    "add_subdirectory": {
      "parameters": [
        {
          "name": "subdirectory_name",
          "type": "string",
          "required": true
        }
      ],
      "command": "mkdir",
      "voidAction": true,
      "noCache": true
    },
    "delete_directory": {
      "parameters": [
        {
          "name": "directory",
          "type": "file",
          "required": true
        }
      ],
      "command": "rm -rf",
      "voidAction": true
    },
    "move": {
      "parameters": [
        {
          "name": "source",
          "type": "file",
          "required": true
        },
        {
          "name": "destination",
          "type": "string",
          "required": true
        }
      ],
      "command": "mv",
      "voidAction": true
    },
    "copy": {
      "parameters": [
        {
          "name": "source",
          "type": "string",
          "required": true
        },
        {
          "name": "destination",
          "type": "string",
          "required": true
        },
        {
		  "name" : "recursive",
		  "type" : "flag",
		  "prefix" : "-r"
        }
      ],
      "command": "cp",
      "voidAction": true
    },
    "mkdirp": {
      "parameters" : [
        {
          "name": "directory",
          "type": "directory",
          "required": true
        }
      ],
      "js": "mkdirp",
      "voidAction": true
    },
    "create_directory": {
      "parameters": [
        {
          "name": "directory",
          "type": "string",
          "required": true
        }
      ],
      "command": "mkdir",
      "voidAction": true
    },
    "write_binary": {
      "parameters": [
        {
          "name": "file_name",
          "type": "string",
          "required": true
        },
        {
          "name": "base64data",
          "type": "string",
          "required": true
        }
      ],
      "js": "write_binary",
      "voidAction": true
    },
    "tail": {
      "parameters": [
        {
          "name": "follow",
          "type": "flag",
          "prefix" : "-f -n +1"
        },
        {
          "name": "file",
          "type": "file"
        }
      ],
      "command": "tail",
      "voidAction": true
    },
    "ls": {
      "parameters": [
        {
          "name": "directory",
          "type": "directory",
          "required": true
        }
      ],
      "permissions" : 0,
      "js": "ls",
      "voidAction": true
    },
    "getRootDir": {
      "parameters": [
      ],
      "js": "getRootDir",
      "voidAction": true
    },
    "getRelativePath": {
      "parameters": [
        {
          "name" : "path",
          "type" : "string",
          "required" : true
        }
      ],
      "js": "getRelativePath",
      "voidAction": true
    },
    "exists": {
      "parameters": [
        {
          "name": "path",
          "type": "file",
          "required": true
        }
      ],
      "permissions" : 0,
      "js": "exists",
      "voidAction": true
    },
    "write_string": {
      "parameters": [
        {
          "name": "file_name",
          "type": "string",
          "required": true
        },
        {
          "name": "data",
          "type": "string",
          "required": true
        }
      ],
      "js": "write_string",
      "voidAction": true
    },
    "unzip_file": {
      "parameters": [
        {
          "name": "input_file",
          "type": "file",
          "required": true
        },
        {
          "name": "destination",
          "type": "string",
          "prefix": "-d ",
          "required": true
        }
      ],
      "command": "unzip",
      "voidAction": true
    }
  }
}
